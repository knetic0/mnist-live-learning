{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "dist/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "^/train$",
      "methods": ["POST"],
      "dest": "index.js"
    },
    {
      "src": "^/predict$",
      "methods": ["POST"],
      "dest": "index.js"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "^(?:/)?(.*)$",
      "dest": "dist/$1"
    },
    {
      "src": "^/(.*)$",
      "dest": "dist/index.html"
    }
  ]
}